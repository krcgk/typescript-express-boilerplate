{
  "name": "ts-ioc-api",
  "version": "1.0.0",
  "description": "Typescript IoC based Api Project",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Build,Api,Web,Socket,Cron,Queue\" -c \"red.bold,magenta.bold,yellow.bold,blue.bold,cyan.bold,green.bold\" \"npm run watch:build\" \"npm run watch:api\" \"npm run watch:web\" \"npm run watch:socket\" \"npm run watch:cron\" \"npm run watch:queue\"",
    "watch:build": "tsc -w -p tsconfig.build.json",
    "watch:api": "nodemon  build/main.js --application=api",
    "watch:socket": "nodemon build/main.js --application=socket",
    "watch:cron": "nodemon build/main.js --application=cron",
    "watch:queue": "nodemon build/main.js --application=queue",
    "watch:web": "nodemon build/main.js --application=web",
    "watch:test": "jest --watch"
  },
  "author": "GÃ¶khan Karaca <mail@gokhankaraca.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/bull": "^3.14.4",
    "@types/connect-redis": "0.0.16",
    "@types/cors": "^2.8.9",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bull": "^3.20.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^7.4.2",
    "yargs": "^16.2.0"
  }
}
